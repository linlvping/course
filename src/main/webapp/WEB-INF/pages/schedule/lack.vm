<div style="text-align:center"><h3>课时确认（未上够课时）</h3></div>
<form class="form-horizontal" role="form" action="${rc.contextPath}/schedule/dolack" method="post">
  <input type="hidden" name="id" value="$!{schedule.id}"/>
  <div class="control-group">
    <label class="col-sm-2 control-label">日期</label>
    <div class="controls">
      <input type="text" class="form-control" value="$!{schedule.date}" disabled="disabled">
    </div>
  </div>
  <div class="control-group">
    <label class="col-sm-2 control-label">老师</label>
    <div class="controls">
      <input type="text" class="form-control" value="$!{schedule.teacher}" disabled="disabled">
    </div>
  </div>
  <div class="control-group">
    <label class="col-sm-2 control-label">学生</label>
    <div class="controls">
      <input type="text" class="form-control" value="$!{schedule.student}" disabled="disabled">
    </div>
  </div>
  <div class="control-group">
    <label class="col-sm-2 control-label">时间段</label>
    <div class="controls">
      <input type="text" class="form-control" value="$!{schedule.time}" disabled="disabled">
    </div>
  </div>
  <div class="control-group">
    <label class="col-sm-2 control-label">真实课时（分钟）</label>
    <div class="controls">
      <input id="fact" name="fact" type="text" class="form-control" placeholder="数字格式，如:90，注意单位是分钟，不是小时">
    </div>
  </div>
  <div class="control-group">
    <div class="col-sm-offset-2 controls">
      <button type="submit" class="btn btn-default">提交确认</button>
    </div>
  </div>
</form>
